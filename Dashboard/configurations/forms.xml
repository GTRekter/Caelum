<?xml version="1.0" encoding="UTF-8"?>
<!--
TYPES:
- text
- boolean
- datetime
- select
- file
-->
<forms>
    <form name="dog-creation">
        <properties>
            <property name="name" type="text" />
            <property name="gender" type="boolean" />
            <property name="dateOfBirth" type="datetime"  />
            <property name="roi" type="text"  />
            <property name="race" type="select" database-select="SELECT * FROM ib_dog_race" />
            <property name="microchip" type="text"  />
            <property name="color" type="select" database-select="SELECT * FROM ib_dog_colors" />
            <property name="pedegree" type="file"  />
        </properties>
    </form>
</forms>